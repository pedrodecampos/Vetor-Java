
public class vetor {
	
	
	// Funcao para ler os dados dos vetores
	public static void imprimir (int[]vet) {
		System.out.println("Dados Do Vetor:");
		for(int i=0; i < vet.length;i++)
		{
			System.out.println("Posicao  " + i + "-->" + vet[i]);
		}
	}
	
	// Funcao para multiplicar em 5 os numeros dos vetores
	public static void mudar(int[] vet) {
		for(int i = 0; i < vet.length;i++)
			vet[i] = vet[i] * 5;
	}
	
	// main 
public static void main(String[] args) {		
 int[] vet;
 vet = new int[5]; 
 for(int i =0; i < vet.length;i++)
	vet[i]= i;
    imprimir(vet); // <-- PadrÃ£o ao incrimento 
    mudar(vet); // <-- Chamando Funcao que muda os valores do vetor
    imprimir(vet); // * 5 <--
	}

}
